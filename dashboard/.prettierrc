{
	"printWidth": 100,
	"trailingComma": "none",
	"singleQuote": true,
	"arrowParens": "always",
	"semi": true,
	"jsxBracketSameLine": true,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": true,
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"tabWidth": 2,
	"useTabs": true
}
